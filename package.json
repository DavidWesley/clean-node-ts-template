{
  "private": true,
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "dev": "cross-env NODE_ENV=development tsx watch src/main.ts",
    "build": "tsup --config tsup.config.ts src",
    "test": "vitest --watch false",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint ./src --diagnostic-level=error",
    "format": "biome format ./src --write --diagnostic-level=warn",
    "prepare": "husky"
  },
  "author": {
    "name": "David Wesley",
    "email": "contato.dwesley@outlook.com",
    "url": "https://github.com/DavidWesley"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@tsconfig/node-lts": "^20.1.1",
    "@tsconfig/strictest": "^2.0.3",
    "@types/node": "^20.11.28",
    "@vitest/coverage-v8": "^1.4.0",
    "cross-env": "^7.0.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
